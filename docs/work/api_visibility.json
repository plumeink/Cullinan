{
  "cullinan.app": {
    "allowlist": [
      "CullinanApplication",
      "create_app"
    ]
  },
  "cullinan.application": {
    "allowlist": [
      "run"
    ]
  },
  "cullinan.core": {
    "allowlist": [
      "Inject",
      "InjectByName",
      "injectable",
      "inject_constructor",
      "InjectionRegistry",
      "ProviderRegistry",
      "SingletonScope",
      "TransientScope",
      "RequestScope",
      "create_context",
      "destroy_context",
      "get_injection_registry",
      "reset_injection_registry",
      "get_request_scope",
      "get_singleton_scope",
      "get_transient_scope",
      "get_current_context",
      "set_current_context",
      "get_context_value",
      "set_context_value"
    ]
  },
  "cullinan.controller": {
    "allowlist": [
      "controller",
      "get_controller_registry",
      "get_header_registry",
      "request_resolver",
      "response_build",
      "reset_controller_registry"
    ]
  },
  "cullinan.service": {
    "allowlist": [
      "Service",
      "ServiceRegistry",
      "get_service_registry",
      "reset_service_registry",
      "service"
    ]
  }
}

